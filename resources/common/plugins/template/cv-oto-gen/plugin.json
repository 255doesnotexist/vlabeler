{
  "name": "cv-oto-gen",
  "version": 2,
  "type": "template",
  "displayedName": "CV oto generator",
  "author": "sdercolin",
  "description": "Generate template for CV oto.",
  "supportedLabelFileExtension": "ini",
  "inputFileExtension": null,
  "parameters": {
    "list": [
      {
        "type": "float",
        "name": "offset",
        "label": "Offset (ms)",
        "defaultValue": 100,
        "min": 0
      },
      {
        "type": "float",
        "name": "overlap",
        "label": "Overlap (ms)",
        "defaultValue": 30
      },
      {
        "type": "float",
        "name": "preutterance",
        "label": "Preutterance (ms)",
        "defaultValue": 60,
        "min": 0
      },
      {
        "type": "float",
        "name": "fixed",
        "label": "Fixed (ms)",
        "defaultValue": 100,
        "min": 0
      },
      {
        "type": "float",
        "name": "cutoff",
        "label": "Cutoff (ms)",
        "defaultValue": -1000
      }
    ]
  },
  "scriptFiles": [
    "cv-oto-gen.js"
  ]
}
