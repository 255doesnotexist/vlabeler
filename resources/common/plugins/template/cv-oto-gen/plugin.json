{
  "name": "cv-oto-gen",
  "version": 4,
  "type": "template",
  "displayedName": {
    "en": "CV oto generator",
    "zh": "单独音 oto 生成器"
  },
  "author": "sdercolin",
  "email": "sder.colin@gmail.com",
  "description": {
    "en": "Generate template for CV oto.",
    "zh": "生成单独音 oto 模板。"
  },
  "website": "https://github.com/sdercolin/vlabeler/tree/main/resources/common/plugins/template/cv-oto-gen",
  "supportedLabelFileExtension": "ini",
  "inputFileExtension": null,
  "parameters": {
    "list": [
      {
        "type": "float",
        "name": "offset",
        "label": "Offset (ms)",
        "defaultValue": 100,
        "min": 0
      },
      {
        "type": "float",
        "name": "overlap",
        "label": "Overlap (ms)",
        "defaultValue": 30
      },
      {
        "type": "float",
        "name": "preutterance",
        "label": "Preutterance (ms)",
        "defaultValue": 60,
        "min": 0
      },
      {
        "type": "float",
        "name": "fixed",
        "label": "Fixed (ms)",
        "defaultValue": 100,
        "min": 0
      },
      {
        "type": "float",
        "name": "cutoff",
        "label": "Cutoff (ms)",
        "defaultValue": -1000
      }
    ]
  },
  "scriptFiles": [
    "cv-oto-gen.js"
  ]
