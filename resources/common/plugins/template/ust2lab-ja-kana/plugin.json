{
  "name": "ust2lab-ja-kana",
  "version": 3,
  "type": "template",
  "displayedName": "Lab generator with ust (ja-kana)",
  "author": "sdercolin",
  "email": "sder.colin@gmail.com",
  "description": "Parse ust with Japanese kana lyrics to sinsy lab style entries. Dictionary powered by https://github.com/oatsu-gh/oto2lab.",
  "supportedLabelFileExtension": "lab",
  "inputFileExtension": "ust",
  "requireInputFile": true,
  "parameters": {
    "list": [
      {
        "type": "integer",
        "name": "overlap",
        "label": "Overlap (ms)",
        "defaultValue": 50,
        "min": 10
      },
      {
        "type": "integer",
        "name": "vowelDelay",
        "label": "Vowel delay when semivowel exists (ms)",
        "defaultValue": 30,
        "min": 10
      }
    ]
  },
  "resourceFiles": [
    "dict.txt"
  ],
  "scriptFiles": [
    "ust2lab-ja-kana.js"
  ]
}
