{
  "name": "lab2audacity",
  "version": 2,
  "type": "template",
  "displayedName": {
    "en": "Sinsy lab format to Audacity Labels",
    "zh": "Sinsy lab 格式转换为 Audacity 标签"
  },
  "author": "sdercolin",
  "email": "sder.colin@gmail.com",
  "description": "Convert Sinsy lab format to Audacity Labels. Can be used on other similar formats with configuration.",
  "website": "https://github.com/sdercolin/vlabeler/tree/main/resources/common/plugins/template/lab2audacity",
  "supportedLabelFileExtension": "txt",
  "inputFileExtension": "lab",
  "requireInputFile": true,
  "outputRawEntry": true,
  "parameters": {
    "list": [
      {
        "type": "float",
        "name": "unit",
        "label": {
          "en": "Time unit (s)",
          "zh": "时间单位（秒）"
        },
        "defaultValue": 0.0000001,
        "min": 0
      }
    ]
  },
  "scriptFiles": [
    "lab2audacity.js"
  ]
}
