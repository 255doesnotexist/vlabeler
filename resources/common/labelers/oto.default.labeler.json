{
  "name": "oto.default",
  "version": 4,
  "extension": "ini",
  "defaultInputFilePath": "oto.ini",
  "displayedName": "UTAU oto labeler",
  "author": "sdercolin",
  "description": "Default labeler for UTAU oto. Overlap cannot be negative.",
  "defaultValues": [
    100,
    400,
    300,
    200,
    500
  ],
  "defaultExtras": [
    "500"
  ],
  "fields": [
    {
      "name": "fixed",
      "label": "Fixed",
      "color": "#0000ff",
      "height": 0.5,
      "dragBase": false,
      "filling": "start",
      "constraints": []
    },
    {
      "name": "preu",
      "label": "Preu",
      "color": "#ff0000",
      "height": 0.6667,
      "dragBase": true,
      "filling": null,
      "constraints": [
        {
          "min": null,
          "max": 0
        }
      ]
    },
    {
      "name": "ovl",
      "label": "Ovl",
      "color": "#00ff00",
      "height": 0.8333,
      "dragBase": false,
      "filling": null,
      "constraints": [
        {
          "min": null,
          "max": 0
        }
      ]
    }
  ],
  "extraFieldNames": [
    "rawRight"
  ],
  "lockedDrag": {
    "useDragBase": true,
    "useStart": false
  },
  "properties": [
    {
      "name": "left",
      "displayedName": "Offset",
      "value": "{start}"
    },
    {
      "name": "fixed",
      "displayedName": "Fixed",
      "value": "{fixed} - {start}"
    },
    {
      "name": "right",
      "displayedName": "Cutoff",
      "value": "{start} - {end} if {end} > 0 else float({rawRight})"
    },
    {
      "name": "preu",
      "displayedName": "Preutterance",
      "value": "{preu} - {start}"
    },
    {
      "name": "ovl",
      "displayedName": "Overlap",
      "value": "{ovl} - {start}"
    }
  ],
  "parser": {
    "defaultEncoding": "Shift-JIS",
    "extractionPattern": "(.*)\\.wav=(.*),(.*),(.*),(.*),(.*),(.*)",
    "variableNames": [
      "sample",
      "name",
      "left",
      "fixed",
      "right",
      "preu",
      "ovl"
    ],
    "scripts": [
      "if(left != ''):",
      "    start = float(left)",
      "points = []",
      "if(fixed != ''):",
      "    points.append(float(fixed) + start)",
      "if(right != ''):",
      "    right = float(right)",
      "    rawRight = right # for restoring from a non-negative value (distance to sample end)",
      "    if(right < 0):",
      "        end = start - right",
      "    else:",
      "        end = -right",
      "if(preu != ''):",
      "    points.append(float(preu) + start)",
      "if(ovl != ''):",
      "    ovl = float(ovl)",
      "    if(ovl < 0):",
      "        ovl = 0",
      "    points.append(ovl + start)"
    ]
  },
  "writer": {
    "format": "{sample}.wav={name},{left},{fixed},{right},{preu},{ovl}"
  }
}