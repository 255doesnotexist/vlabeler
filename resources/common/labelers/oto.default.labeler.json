{
  "name": "oto.default",
  "version": 1,
  "displayedName": "UTAU oto labeler",
  "author": "sdercolin",
  "description": "Default oto labeler (Overlap cannot be negative).",
  "fields": [
    {
      "name": "Fixed",
      "abbr": "Fixed",
      "color": "#0000ff",
      "height": 0.5,
      "dragBase": false,
      "filling": -2,
      "constraints": []
    },
    {
      "name": "Preutterance",
      "abbr": "Preu",
      "color": "#ff0000",
      "height": 0.6667,
      "dragBase": true,
      "filling": null,
      "constraints": [
        {
          "min": null,
          "max": 0
        }
      ]
    },
    {
      "name": "Overlap",
      "abbr": "Ovl",
      "color": "#00ff00",
      "height": 0.8333,
      "dragBase": false,
      "filling": null,
      "constraints": [
        {
          "min": null,
          "max": 0
        }
      ]
    }
  ],
  "defaultValues": [
    100,
    400,
    300,
    200,
    500
  ],
  "properties": [
    {
      "name": "Offset",
      "value": "{-2}"
    },
    {
      "name": "Fixed",
      "value": "{0}-{-2}"
    },
    {
      "name": "Blank",
      "value": "{-2}-{-1}"
    },
    {
      "name": "Preutterance",
      "value": "{1}-{-2}"
    },
    {
      "name": "Overlap",
      "value": "{2}-{-2}"
    }
  ],
  "lockedDrag": {
    "useDragBase": true,
    "useStart": true
  },
  "parser": {
    "extension": "ini",
    "defaultInputFilePath": "oto.ini",
    "defaultEncoding": "Shift-JIS",
    "extractionPattern": "(.*)\\.wav=(.*),(.*),(.*),(.*),(.*),(.*)",
    "variableNames": [
      "sample",
      "alias",
      "left",
      "fixed",
      "right",
      "preu",
      "ovl"
    ],
    "parsingScript": [
      "name = alias",
      "start = float(left)",
      "points = []",
      "points.append(float(fixed) + start)",
      "right = float(right)",
      "if(right < 0):",
      "    end = start - right",
      "else:",
      "    end = -right",
      "points.append(float(preu) + start)",
      "points.append(float(ovl) + start)"
    ]
  }
}