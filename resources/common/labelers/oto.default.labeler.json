{
  "name": "oto.default",
  "version": 1,
  "extension": "ini",
  "defaultInputFilePath": "oto.ini",
  "displayedName": "UTAU oto labeler",
  "author": "sdercolin",
  "description": "Default oto labeler (Overlap cannot be negative).",
  "defaultValues": [
    100,
    400,
    300,
    200,
    500
  ],
  "fields": [
    {
      "name": "fixed",
      "label": "Fixed",
      "color": "#0000ff",
      "height": 0.5,
      "dragBase": false,
      "filling": "start",
      "constraints": []
    },
    {
      "name": "preu",
      "label": "Preu",
      "color": "#ff0000",
      "height": 0.6667,
      "dragBase": true,
      "filling": null,
      "constraints": [
        {
          "min": null,
          "max": 0
        }
      ]
    },
    {
      "name": "ovl",
      "label": "Ovl",
      "color": "#00ff00",
      "height": 0.8333,
      "dragBase": false,
      "filling": null,
      "constraints": [
        {
          "min": null,
          "max": 0
        }
      ]
    }
  ],
  "extraFieldNames": [
    "rawRight"
  ],
  "lockedDrag": {
    "useDragBase": true,
    "useStart": false
  },
  "parser": {
    "defaultEncoding": "Shift-JIS",
    "extractionPattern": "(.*)\\.wav=(.*),(.*),(.*),(.*),(.*),(.*)",
    "variableNames": [
      "sample",
      "alias",
      "left",
      "fixed",
      "right",
      "preu",
      "ovl"
    ],
    "scripts": [
      "name = alias",
      "start = float(left)",
      "points = []",
      "points.append(float(fixed) + start)",
      "right = float(right)",
      "if(right < 0):",
      "    end = start - right",
      "else:",
      "    end = -right",
      "points.append(float(preu) + start)",
      "points.append(float(ovl) + start)",
      "rawRight = right"
    ]
  },
  "writer": {
    "properties": [
      {
        "name": "left",
        "displayedName": "Offset",
        "value": "{start}"
      },
      {
        "name": "fixed",
        "displayedName": "Fixed",
        "value": "{fixed}-{start}"
      },
      {
        "name": "right",
        "displayedName": "Blank",
        "value": "{start}-{end}"
      },
      {
        "name": "preu",
        "displayedName": "Preutterance",
        "value": "{preu}-{start}"
      },
      {
        "name": "ovl",
        "displayedName": "Overlap",
        "value": "{ovl}-{start}"
      }
    ],
    "scripts": [
      "if(end <=0):",
      "    right = rawRight",
      "output = sample + \".wav=\" + name + \",\" + str(left) + \",\" + str(fixed) + \",\" + str(right) + \",\" + str(preu) + \",\" + str(ovl)"
    ]
  }
}