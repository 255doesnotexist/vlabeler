{
  "name": "oto-plus.default",
  "version": 5,
  "extension": "ini",
  "defaultInputFilePath": "oto.ini",
  "displayedName": "UTAU oto labeler plus",
  "author": "sdercolin",
  "email": "sder.colin@gmail.com",
  "description": "Default labeler for UTAU oto. Left is separated from the default start, so Overlap can be set minus.",
  "website": "https://github.com/sdercolin/vlabeler/tree/main/resources/common/labelers",
  "defaultValues": [
    100,
    400,
    300,
    200,
    100,
    500
  ],
  "defaultExtras": [
    "500"
  ],
  "fields": [
    {
      "name": "fixed",
      "label": "Fixed",
      "color": "#0000ff",
      "height": 0.5,
      "dragBase": false,
      "filling": "left",
      "constraints": [
        {
          "min": 3,
          "max": null
        }
      ],
      "shortcutIndex": 4
    },
    {
      "name": "preu",
      "label": "Preu",
      "color": "#ff0000",
      "height": 0.6667,
      "dragBase": true,
      "filling": null,
      "constraints": [
        {
          "min": 3,
          "max": 0
        }
      ],
      "shortcutIndex": 3
    },
    {
      "name": "ovl",
      "label": "Ovl",
      "color": "#00ff00",
      "height": 0.8333,
      "dragBase": false,
      "filling": null,
      "constraints": [
        {
          "min": null,
          "max": 0
        }
      ],
      "shortcutIndex": 2
    },
    {
      "name": "left",
      "label": "Left",
      "color": "#dba100",
      "height": 0.9,
      "dragBase": false,
      "filling": "start",
      "constraints": [],
      "shortcutIndex": 1
    }
  ],
  "extraFieldNames": [
    "rawRight"
  ],
  "lockedDrag": {
    "useDragBase": true,
    "useStart": false
  },
  "overflowBeforeStart": "AdjustPoint",
  "overflowAfterEnd": "AdjustBorder",
  "properties": [
    {
      "name": "left",
      "displayedName": "Offset",
      "value": "{left}"
    },
    {
      "name": "fixed",
      "displayedName": "Fixed",
      "value": "{fixed} - {left}"
    },
    {
      "name": "right",
      "displayedName": "Cutoff",
      "value": "{end} > 0 ? {left} - {end} : parseFloat({rawRight})"
    },
    {
      "name": "preu",
      "displayedName": "Preutterance",
      "value": "{preu} - {left}"
    },
    {
      "name": "ovl",
      "displayedName": "Overlap",
      "value": "{ovl} - {left}"
    }
  ],
  "parser": {
    "defaultEncoding": "Shift-JIS",
    "extractionPattern": "(.*)\\.wav=(.*),(.*),(.*),(.*),(.*),(.*)",
    "variableNames": [
      "sample",
      "name",
      "left",
      "fixed",
      "right",
      "preu",
      "ovl"
    ],
    "scripts": [
      "offset = 0",
      "if (ovl !== '') {",
      "    ovl = parseFloat(ovl)",
      "    if (ovl < 0) {",
      "        offset = -ovl",
      "    }",
      "}",
      "if (left !== '') {",
      "    left = parseFloat(left)",
      "    start = left - offset",
      "}",
      "points = []",
      "extras = []",
      "if (fixed !== '') {",
      "    fixed = parseFloat(fixed)",
      "    if (fixed < 0) {",
      "        fixed = 0",
      "    }",
      "    points.push(fixed + left)",
      "}",
      "if (right !== '') {",
      "    right = parseFloat(right)",
      "    rawRight = right // for restoring from a non-negative value (distance to sample end)",
      "    extras.push(rawRight)",
      "    if (right < 0) {",
      "        end = left - right",
      "    } else {",
      "        end = -right",
      "    }",
      "}",
      "if (preu !== '') {",
      "    preu = parseFloat(preu)",
      "    if (preu < 0) {",
      "        preu = 0",
      "    }",
      "    points.push(preu + left)",
      "}",
      "if (ovl !== '') {",
      "    points.push(ovl + left)",
      "}",
      "if (left !== '') {",
      "    points.push(left)",
      "}"
    ]
  },
  "writer": {
    "format": "{sample}.wav={name},{left},{fixed},{right},{preu},{ovl}"
  }
}
